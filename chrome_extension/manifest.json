{
  "manifest_version": 3,
  "name": "YouTube RAG Chatbot",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["popup.js"]
    }
  ]
}
